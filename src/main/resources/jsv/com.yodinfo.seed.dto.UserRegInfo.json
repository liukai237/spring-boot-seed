{
  "$schema": "http://json-schema.org/draft-04/schema",
  "type": "object",
  "properties": {
    "tel": {
      "type": "string",
      "pattern": "^[1](([3][0-9])|([4][5,7,9])|([5][^4,6,9])|([6][6])|([7][3,5,6,7,8])|([8][0-9])|([9][8,9]))[0-9]{8}$"
    },
    "vcode": {
      "type": "string"
    }
  },
  "required": [
    "tel",
    "vcode"
  ]
}